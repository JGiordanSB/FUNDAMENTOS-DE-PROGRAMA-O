#include <iostream>
using namespace std;
int main(){
    char op;
    int jog1 {}, jog2 {};
    cin >> op >> jog1 >> jog2;
    int res = (jog1 + jog2) % 2;
    
    if (op == 'i'){
        if (res != 0 ){
            cout << "Venceu\n";
        } else {
            cout << "Perdeu\n";
        } 
    } else if (op == 'p'){
        if (res == 0){
            cout << "Venceu\n";
        } else {
            cout << "Perdeu\n";
        }
    }
} 
